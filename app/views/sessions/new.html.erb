<% provide(:title, 'Login') %>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
      <h3>Welcome back, please Sign In</h3>
      <%= form_tag do %>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
      <br>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
      <br>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
      <%= submit_tag "Login"%>
    <% end %>
  </div>
</div>
